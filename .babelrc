{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "6.10",
                    "esmodules": true
                }
            }
        ]
    ],
    "plugins": [
        ["@babel/plugin-transform-runtime",
          {
            "regenerator": true
          }
        ],
        "@babel/plugin-proposal-object-rest-spread"
      ]
}